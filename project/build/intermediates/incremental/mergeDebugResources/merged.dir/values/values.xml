<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string-array name="aboutDialog">
        <item>About Quick Bart</item>
        <item>Quick BART uses the source code of Open Bart as its base to it follows their GPLv3 license. Code is available on Github &lt;a href=\"https://github.com/kevingil/BART\">https://github.com/kevingil/BART&lt;/a>.&lt;br/>&lt;br/>
              Need help? Email us! &lt;br/>&lt;a href=\"mailto:fancybearstudioinc@gmail.com\">fancybearstudioinc@gmail.com&lt;/a>.
              &lt;h1>Thanks&lt;/h1>
        </item>
    </string-array>
    <string-array name="crashCatchDialog">
        <item>We\'re sorry!</item>
        <item>Quick Bart is misbehaving, and our developers have been notified. If you please, 
              &lt;a href="mailto:fancybearstudioinc@gmail.com">write us&lt;/a> and tell us how you got here!</item>
    </string-array>
    <string-array name="networkErrorDialog">
        <item>Network Error</item>
        <item>Check your network connection.</item>
    </string-array>
