        &lt;h1>Happy Riding&lt;/h1>
        </string>
    <string name="local_service_connected">trip usher at work</string>
    <string name="local_service_disconnected">trip usher off</string>
    <string name="local_service_label">I\'ll vibrate when you need to move</string>
